{
  "name": "@promotedai/react-native-metrics",
  "version": "0.5.1",
  "description": "Promoted metrics logging library for React Native.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "react-native-metrics.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepublishOnly": "git clean -d -x -f ./android && git clean -d -x -f ./ios",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/promotedai/react-native-metrics",
  "author": "Yu-Hong Wang <yu-hong@promoted.ai> (https://github.com/rockapot)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/promotedai/react-native-metrics/issues"
  },
  "homepage": "https://github.com/promotedai/react-native-metrics#readme",
  "publishConfig": {
    "cache": "/tmp/npm",
    "registry": "https://registry.npmjs.org/"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-native": "^0.64.2"
  },
  "devDependencies": {
    "react": "^16.14.0",
    "react-native": "^0.64.2",
    "react-native-builder-bob": "0.18.1",
    "typescript": "^4.3.2"
  }
}
